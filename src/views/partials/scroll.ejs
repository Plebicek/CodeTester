   <div class="sticky top-40 flex justify-evenly flex-col lg:mt-20 lg:pb-10 lg:ml-6 gap-5 lg:max-h-48 ">
            <% sideNav.forEach(nav => { %>
                <% if (nav.name == current) { %>
                    <a href="<%= nav.link %>" class="flex gap-4 items-start">
                        <div class="w-55px flex justify-center h-full items-center">
                            <img src="/images/show-<%= nav.icon %>" class="fill-main" alt="">
                        </div>
                        <p class="font-medium lg:text-3xl text-main" ><%= nav.name %></p>
                    </a>
                <% } else { %>
                    <a href="<%= nav.link %>" class="flex gap-4 items-start">
                        <div class="w-55px flex justify-center h-full items-center">
                            <img src="/images/<%= nav.icon %>" class="" alt="">
                        </div>
                        <p class="font-medium lg:text-3xl text-text-second" ><%= nav.name %></p>
                    </a>
                <% } %>
          
            <% }) %>
        </div> 